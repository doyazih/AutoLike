{
    "manifest_version": 2,
    
    "name": "AutoLike for Instagram",
    "description": "This is auto macro program for Instagram users. Auto page loading and auto like on your all feeds.",
    "version": "1.0",
    
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png" 
    },

    "browser_action": {
        "default_icon": {                    
            "16": "images/icon16.png",           
            "24": "images/icon24.png",           
            "32": "images/icon32.png"            
        },
        "default_title": "AutoLike for Instagram",
        "default_popup": "popup.html"
    },

    "author": "tofufather",
    
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
}